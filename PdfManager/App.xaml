<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PdfManager"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="PdfManager.App"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <System:String x:Key="Other1Name">其他1</System:String>
        <System:String x:Key="Other2Name">其他2</System:String>

        <Style x:Key="BlackIconStyle" TargetType="Image">
            <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
            <Setter Property="Width" Value="16"/>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                </Setter.Value>
            </Setter>
        </Style>
    </Application.Resources>
</Application>
